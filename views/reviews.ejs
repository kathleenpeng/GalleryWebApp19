<% include ../partials/header %>


<div class="row">
  <div class="contactsection">
      
  <h1>Welcome to the review outcome<br>
   
  </h1>
    
           
       
       <br>
        <div class="grid-container">
       <% review.forEach(function(review) { %> 
       
      
           <div class="item">
               <h1 class="itemtitle">Id: <%= review.id %></h1>
               <p class="itemp">Name: <%= review.name %></p>
               <p class="itemp">Comment: <%= review.comment %></p> 
                <p class="itemp">Email: <%= review.email %></p> 
               </div>
      
       
       <% }); %>
        </div>
    </div>

 </div>
<% include ../partials/footer %>
